{
    "manifest_version": 3,
    "name": "Reading time",
    "description": "add reading time to chrome extensions",
    "version": "1.0",
    "icons":{
      "16":"heart.png"
    },
    "action":{
      "default_popup":"popup.html"
    },
    "permissions":["tabs","activeTab"],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
}